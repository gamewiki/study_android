<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sid.almanac"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/almanac"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartActivity"
            android:label="@string/title_activity_almanac" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
    	<activity android:name=".AlmanacActivity" android:label="@string/app_name"></activity>
        <activity android:name="sid.utils.AboutActivity" android:label="@string/app_name" ></activity>
    </application>

</manifest>