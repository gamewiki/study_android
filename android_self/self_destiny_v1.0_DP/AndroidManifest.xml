<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sid.destiny"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <!-- 必须申明的权限 -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <!-- 可选的权限 -->
    <!-- 启用错误分析功能，须添加该权限 -->
    <uses-permission android:name="android.permission.READ_LOGS"/>

    <!-- 可选的权限 -->
    <!-- 使用GPS获取用户精确定位信息 -->
    <!-- <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission> -->


    <!-- 多普的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:icon="@drawable/bz_cg_zb_appname"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- 您从百度网站获取的APP KEY -->
        <meta-data android:name="BaiduMobAd_STAT_ID" android:value="adfc2323d9" />
        <!-- 渠道商编号 -->
        <meta-data android:name="BaiduMobAd_CHANNEL" android:value="Baidu Market" />
        <!-- 是否开启错误日志统计，默认为false -->
        <meta-data android:name="BaiduMobAd_EXCEPTION_LOG" android:value="true" />
        <!-- 日志发送策略，可选值：APP_START、ONCE_A_DAY、SET_TIME_INTERVAL，默认为APP_START -->
        <meta-data android:name="BaiduMobAd_SEND_STRATEGY" android:value="APP_START" />
        <!-- 日志发送策略 为SET_TIME_INTERVAL时，需设定时间间隔(取消下行注释)。取值为1-24的整数，默认为1 -->
        <!-- <meta-data android:name="BaiduMobAd_TIME_INTERVAL" android:value="2" /> -->
        <!-- 日志仅在wifi网络下发送，默认为false -->
        <meta-data android:name="BaiduMobAd_ONLY_WIFI" android:value="false" />
        <!-- 是否获取基站位置信息 -->
        <meta-data android:name="BaiduMobAd_CELL_LOCATION" android:value="true" />

        
        <!-- 多普广告的activity -->
        <activity android:name="com.waps.OffersWebView" android:configChanges="keyboardHidden|orientation"/>
		<!--以下属性为应用ID,从万普后台获取-->
		<meta-data android:name="WAPS_ID" android:value="a7e0ddcf7a2a28da40812ad986de8f91" />
		<!--以下属性为分发渠道ID,编码表参见本文档末附表-->
		<meta-data android:name="WAPS_PID" android:value="qq" /> 
		
		
        <activity android:name=".DestinyActivity"
            android:label="@string/title_activity_destiny" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sid.utils.AboutActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.cg.CGSynopsisActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.cg.CGUserInfoActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.cg.CGResultActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.cg.CGResultDesActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.cg.CGUserListActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.gylq.LQSynopsisActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.gylq.LQStartActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
        <activity
            android:name="sid.gylq.LQResultActivity"
            android:label="@string/title_activity_destiny" >
        </activity>
    </application>

</manifest>