<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <Button 
        android:id="@+id/geoButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/geo"/>
    <Button 
        android:id="@+id/reverseGeoButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/reverseGeo"/>
    <Button 
        android:id="@+id/geowebButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/geoweb"/>
    <Button 
        android:id="@+id/reverseGeowebButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/reverseGeoweb"/>

</LinearLayout>
